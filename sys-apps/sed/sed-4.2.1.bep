DESCRIPTION="sed - a stream editor" 
HOMEPAGE="http://www.gnu.org/software/sed" 
SRC_URI="http://ftp.gnu.org/gnu/sed/sed-4.2.1.tar.gz"
REVISION="1"
STATUS_HAIKU="stable"
DEPEND=""
BUILD {
	cd sed-4.2.1
	autoconf
	./configure --prefix=/boot/common
	make
}

INSTALL {
	cd sed-4.2.1
	make install
	make check
}
