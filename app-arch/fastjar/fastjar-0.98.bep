DESCRIPTION="A jar program written in C"
HOMEPAGE="https://savannah.nongnu.org/projects/fastjar"
SRC_URI="http://mirror.its.uidaho.edu/pub/savannah/fastjar/fastjar-0.98.tar.gz"
REVISION="2"
STATUS_HAIKU="stable"
DEPEND=""
BUILD {
	cd fastjar-0.98
	autoreconf -vfi
	configure --prefix=/boot/common
	make
}

INSTALL {
	cd fastjar-0.98
	make install
}
