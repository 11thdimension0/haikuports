DESCRIPTION="jpeg" 
HOMEPAGE="http://www.ijg.org" 
SRC_URI="http://www.ijg.org/files/jpegsrc.v6b.tar.gz"
REVISION="1"
STATUS_R5="untested"
STATUS_BONE="untested"
STATUS_HAIKU="stable"
DEPEND=""
BUILD {
	cd jpeg-6b
	aclocal
        libtoolize --force --copy
        automake --add-missing
        autoconf
        ./configure --prefix=/boot/common --enable-shared
        make 
}

INSTALL {
	cd jpeg-6b
	make install
}
